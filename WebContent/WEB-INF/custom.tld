<taglib>
	<tlib-version>1.0</tlib-version>
	<jsp-version>2.0</jsp-version>
	<short-name>Example TLD with Body</short-name>

	<tag>
		<name>Date</name>
		<tag-class>ua.nure.hrybeniuk.finalProject.web.customTags.DateTag
		</tag-class>
